{
  "name": "ant-design-pro",
  "version": "1.0.0",
  "private": true,
  "description": "An out-of-box UI solution for enterprise applications",
  "scripts": {
    "build": "umi build",
    "start": "umi dev",
    "lint": "eslint . --ext js --ext jsx --ext ts --ext tsx",
    "lint-staged": "lint-staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged"
    }
  },
  "pre-commit": [
    "lint-staged"
  ],
  "lint-staged": {
    "./**/*.less": [
      "prettier --write",
      "stylelint --fix",
      "git add"
    ],
    "./**/*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.0.0",
    "@ant-design/pro-layout": "^5.0.8",
    "@types/classnames": "^2.2.10",
    "antd": "^4.0.0",
    "bizcharts": "^3.5.3-beta.0",
    "bizcharts-plugin-slider": "^2.1.1-beta.1",
    "classnames": "^2.2.6",
    "debug": "^4.1.1",
    "eslint": "^6.8.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "express-winston": "^4.0.3",
    "gg-editor": "^2.0.2",
    "lodash": "^4.17.11",
    "marked": "^1.0.0",
    "moment": "^2.25.3",
    "mysql": "^2.18.1",
    "omit.js": "^1.0.2",
    "path-to-regexp": "2.4.0",
    "prop-types": "^15.5.10",
    "qs": "^6.9.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-fittext": "^1.0.0",
    "react-helmet-async": "^1.0.4",
    "umi": "^3.0.14",
    "umi-request": "^1.0.8",
    "use-merge-value": "^1.0.1"
  },
  "devDependencies": {
    "del-cli": "^3.0.0",
    "eslint": "^6.8.0",
    "lint-staged": "^10.0.0",
    "pre-commit": "^1.2.2",
    "prettier": "^2.0.1",
    "stylelint": "^13.0.0",
    "typescript": "^3.7.5",
    "@umijs/preset-react": "^1.4.8",
    "@umijs/fabric": "^2.0.5",
    "cross-port-killer": "^1.1.1",
    "husky": "^4.0.7",
    "jsdom-global": "^3.0.2"
  },
  "optionalDependencies": {
    "puppeteer": "^2.0.0"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "checkFiles": [
    "src/**/*.js*",
    "src/**/*.ts*",
    "src/**/*.less",
    "config/**/*.js*",
    "scripts/**/*.js"
  ]
}
